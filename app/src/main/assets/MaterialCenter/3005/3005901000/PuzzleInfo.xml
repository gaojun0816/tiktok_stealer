<?xml version="1.0" encoding="utf-8"?>
<puzzle>
    <resId>3005901000</resId>
    <version>1</version>
    <name>自由拼图模板1</name>
    <iconPath>thumbnail.post</iconPath>
    <width>1000</width>
    <height>1500</height>
    <!-- 图片的缩放形式： 0：居中适配； 1：宽度固定，长度自适应，超过视图高度可上下滑动；2：高度固定，宽度自适应-->
    <scaleType>0</scaleType>
    <photoAmount>1</photoAmount>
    <formatAmount>1</formatAmount>
    <backgroundColor>#fffffffFF</backgroundColor>
    <backgroundImagePath />
    <backgroundTile />

    <puzzlePieces>
        <photoPuzzle>
            <maskPath />
            <filterPath />
            <backgroundImagePath />
            <backgroundImageInnerFrame />
            <frameRectArray>
                <frameRect>{{0,0},{1000,1500}}</frameRect>
            </frameRectArray>
            <matrixTransformContentByTouch>true</matrixTransformContentByTouch>
            <confinedInCell>false</confinedInCell>
            <contentMode>1</contentMode>
            <photoIndex>0</photoIndex>

            <rotate>-15</rotate>
            <scaleRatio>0.7</scaleRatio>
            <centerPointHorizontalBias>0</centerPointHorizontalBias>
            <centerPointVerticalBias>0</centerPointVerticalBias>
        </photoPuzzle>
    </puzzlePieces>
</puzzle>
